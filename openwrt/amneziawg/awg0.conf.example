# AmneziaWG Configuration Template
# /etc/amneziawg/awg0.conf
#
# Replace ALL placeholder values before use
# chmod 600 /etc/amneziawg/awg0.conf

[Interface]
# Your private key (generate with: amneziawg genkey)
PrivateKey = YOUR_PRIVATE_KEY_BASE64

# =============================================================================
# AmneziaWG Obfuscation Parameters
# =============================================================================
# These parameters add client-side obfuscation to disguise WireGuard traffic.
# The obfuscation happens LOCALLY - servers don't need to support AmneziaWG.
#
# IMPORTANT: Most VPN providers (Mullvad, IVPN, Proton, etc.) do NOT provide
# AmneziaWG parameters because they use standard WireGuard servers.
#
# The values below are WORKING DEFAULTS compatible with ANY standard WireGuard
# server. Source: wgtunnel's AmneziaWG compatibility mode.
# https://github.com/zaneschepke/wgtunnel
#
# Use these as-is - no need to get values from your provider.

# Jc: Junk packets during handshake (DPI obfuscation)
Jc = 4

# Jmin/Jmax: Size range of junk packets in bytes
Jmin = 40
Jmax = 70

# S1/S2: Init packet padding (0 for standard WireGuard compatibility)
S1 = 0
S2 = 0

# H1-H4: Header obfuscation (sequential for standard WireGuard compatibility)
H1 = 1
H2 = 2
H3 = 3
H4 = 4

[Peer]
# VPN server's public key (from your provider)
PublicKey = SERVER_PUBLIC_KEY_BASE64

# Optional: Pre-shared key for post-quantum security
# PresharedKey = PRESHARED_KEY_BASE64

# Route all IPv4 and IPv6 through tunnel
# For split tunneling, specify only certain ranges
AllowedIPs = 0.0.0.0/0, ::/0

# VPN server address and port
# Use IP address, not hostname (avoids DNS dependency at startup)
Endpoint = VPN_SERVER_IP:51820

# Send keepalive every 25 seconds
# Required for NAT traversal, prevents connection timeout
PersistentKeepalive = 25
